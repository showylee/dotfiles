{
	"autoClosingPairs": [
		// markdown
		// fix https://github.com/johnsoncodehk/volar/issues/1428
		{
			"open": "```",
			"close": "\n\n```",
			"notIn": [
				"string",
				"comment"
			]
		},
		// html
		{
			"open": "{",
			"close": "}"
		},
		{
			"open": "[",
			"close": "]"
		},
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "'",
			"close": "'"
		},
		{
			"open": "\"",
			"close": "\""
		},
		{
			"open": "<!--",
			"close": "-->",
			"notIn": [
				"comment",
				"string"
			]
		},
		// javascript
		{
			"open": "`",
			"close": "`",
			"notIn": [
				"string",
				"comment"
			]
		},
		{
			"open": "/**",
			"close": " */",
			"notIn": [
				"string"
			]
		}
	],
	"colorizedBracketPairs": [
		[
			"{{",
			"}}"
		],
	]
}